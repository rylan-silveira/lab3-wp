<%- include('header'); -%>

<div class="content">

  <h1>Selected articles</h1>

  <table id="catalog">

    <thead>

      <tr>

        <th>Name</th>

        <th scope="col">Reference</th>

        <th scope="col">Category</th>

        <th scope="col">Price</th>

      </tr>

    </thead>

    <tbody>

      <% { %>

      <tr>

        <th scope="row"><%= product.name %></th>

        <td><%= product.reference %></td>

        <td><%= product.category %></td>

        <td><%= product.price %>AED</td>

      </tr>

      <% } %>

    </tbody>

  </table>

</div>

<br />

<%- include('footer'); -%>